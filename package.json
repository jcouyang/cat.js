  "license": "MIT",
  "devDependencies": {
    "browserify": "^11.2.0",
    "mocha": "^2.3.3",
    "should": "^7.1.0",
    "tsc": "^1.20150623.0",
    "typedoc": "^0.3.12",
    "typescript": "^1.6.2"
  }
}
